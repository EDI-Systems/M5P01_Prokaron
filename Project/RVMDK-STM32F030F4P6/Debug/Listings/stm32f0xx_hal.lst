N  __IO uint32_t RTOR;   /*!< USART Receiver Time Out register,         Address offset: 0x14 */  
X  volatile uint32_t RTOR;      
N  __IO uint32_t RQR;    /*!< USART Request register,                   Address offset: 0x18 */
