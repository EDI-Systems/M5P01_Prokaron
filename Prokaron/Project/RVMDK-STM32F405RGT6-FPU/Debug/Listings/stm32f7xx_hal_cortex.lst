N  /* Check the parameters */
N  assert_param(IS_MPU_REGION_NUMBER(MPU_Init->Number));
X  ((void)0);
N  assert_param(IS_MPU_REGION_ENABLE(MPU_Init->Enable));
X  ((void)0);
N
N  /* Set the Region number */
N  MPU->RNR = MPU_Init->Number;
X  ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RNR = MPU_Init->Number;
N
N  if ((MPU_Init->Enable) != RESET)
N  {
N    /* Check the parameters */
N    assert_param(IS_MPU_INSTRUCTION_ACCESS(MPU_Init->DisableExec));
X    ((void)0);
N    assert_param(IS_MPU_REGION_PERMISSION_ATTRIBUTE(MPU_Init->AccessPermission));
X    ((void)0);
N    assert_param(IS_MPU_TEX_LEVEL(MPU_Init->TypeExtField));
X    ((void)0);
N    assert_param(IS_MPU_ACCESS_SHAREABLE(MPU_Init->IsShareable));
X    ((void)0);
N    assert_param(IS_MPU_ACCESS_CACHEABLE(MPU_Init->IsCacheable));
X    ((void)0);
N    assert_param(IS_MPU_ACCESS_BUFFERABLE(MPU_Init->IsBufferable));
X    ((void)0);
N    assert_param(IS_MPU_SUB_REGION_DISABLE(MPU_Init->SubRegionDisable));
X    ((void)0);
N    assert_param(IS_MPU_REGION_SIZE(MPU_Init->Size));
X    ((void)0);
N    
N    MPU->RBAR = MPU_Init->BaseAddress;
X    ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = MPU_Init->BaseAddress;
N    MPU->RASR = ((uint32_t)MPU_Init->DisableExec             << MPU_RASR_XN_Pos)   |
X    ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = ((uint32_t)MPU_Init->DisableExec             << 28U)   |
N                ((uint32_t)MPU_Init->AccessPermission        << MPU_RASR_AP_Pos)   |
X                ((uint32_t)MPU_Init->AccessPermission        << 24U)   |
N                ((uint32_t)MPU_Init->TypeExtField            << MPU_RASR_TEX_Pos)  |
X                ((uint32_t)MPU_Init->TypeExtField            << 19U)  |
N                ((uint32_t)MPU_Init->IsShareable             << MPU_RASR_S_Pos)    |
X                ((uint32_t)MPU_Init->IsShareable             << 18U)    |
N                ((uint32_t)MPU_Init->IsCacheable             << MPU_RASR_C_Pos)    |
X                ((uint32_t)MPU_Init->IsCacheable             << 17U)    |
N                ((uint32_t)MPU_Init->IsBufferable            << MPU_RASR_B_Pos)    |
X                ((uint32_t)MPU_Init->IsBufferable            << 16U)    |
N                ((uint32_t)MPU_Init->SubRegionDisable        << MPU_RASR_SRD_Pos)  |
X                ((uint32_t)MPU_Init->SubRegionDisable        << 8U)  |
N                ((uint32_t)MPU_Init->Size                    << MPU_RASR_SIZE_Pos) |
X                ((uint32_t)MPU_Init->Size                    << 1U) |
N                ((uint32_t)MPU_Init->Enable                  << MPU_RASR_ENABLE_Pos);
X                ((uint32_t)MPU_Init->Enable                  << 0U);
N  }
N  else
N  {
N    MPU->RBAR = 0x00;
X    ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0x00;
N    MPU->RASR = 0x00;
X    ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = 0x00;
N  }
