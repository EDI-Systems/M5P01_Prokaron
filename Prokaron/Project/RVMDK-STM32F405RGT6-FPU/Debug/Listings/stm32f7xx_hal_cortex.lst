N/**
N  \brief   Enable D-Cache
N  \details Turns on D-Cache
N  */
N__STATIC_INLINE void SCB_EnableDCache (void)
Xstatic __inline void SCB_EnableDCache (void)
N{
N  #if (__DCACHE_PRESENT == 1U)
X  #if (1 == 1U)
N    uint32_t ccsidr;
N    uint32_t sets;
N    uint32_t ways;
N
