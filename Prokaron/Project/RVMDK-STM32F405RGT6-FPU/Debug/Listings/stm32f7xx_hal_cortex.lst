N/**
N  * @brief  Configures the SysTick clock source.
N  * @param  CLKSource: specifies the SysTick clock source.
N  *          This parameter can be one of the following values:
N  *             @arg SYSTICK_CLKSOURCE_HCLK_DIV8: AHB clock divided by 8 selected as SysTick clock source.
N  *             @arg SYSTICK_CLKSOURCE_HCLK: AHB clock selected as SysTick clock source.
N  * @retval None
N  */
Nvoid HAL_SYSTICK_CLKSourceConfig(uint32_t CLKSource)
N{
N  /* Check the parameters */
N  assert_param(IS_SYSTICK_CLK_SOURCE(CLKSource));
X  ((void)0);
N  if (CLKSource == SYSTICK_CLKSOURCE_HCLK)
X  if (CLKSource == ((uint32_t)0x00000004U))
N  {
N    SysTick->CTRL |= SYSTICK_CLKSOURCE_HCLK;
X    ((SysTick_Type *) ((0xE000E000UL) + 0x0010UL) )->CTRL |= ((uint32_t)0x00000004U);
N  }
N  else
N  {
N    SysTick->CTRL &= ~SYSTICK_CLKSOURCE_HCLK;
X    ((SysTick_Type *) ((0xE000E000UL) + 0x0010UL) )->CTRL &= ~((uint32_t)0x00000004U);
N  }
N}
