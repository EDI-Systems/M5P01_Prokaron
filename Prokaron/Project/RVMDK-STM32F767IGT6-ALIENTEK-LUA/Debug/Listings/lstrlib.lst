N  else {
N    const char *init;  /* to search for a '*s2' inside 's1' */
N    l2--;  /* 1st char will be checked by 'memchr' */
N    l1 = l1-l2;  /* 's2' cannot be found after that */
N    while (l1 > 0 && (init = (const char *)memchr(s1, *s2, l1)) != NULL) {
X    while (l1 > 0 && (init = (const char *)memchr(s1, *s2, l1)) != 0) {
N      init++;   /* 1st char is already checked */
N      if (memcmp(init, s2+1, l2) == 0)
N        return init-1;
N      else {  /* correct 'l1' and 's1' to try again */
N        l1 -= init-s1;
