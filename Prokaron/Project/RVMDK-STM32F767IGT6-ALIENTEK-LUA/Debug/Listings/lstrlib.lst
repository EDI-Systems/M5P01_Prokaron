N               matchbracketclass(uchar(*s), p, ep - 1)) {
X               matchbracketclass(((unsigned char)(*s)), p, ep - 1)) {
N              p = ep; goto init;  /* return match(ms, s, ep); */
N            }
N            s = NULL;  /* match failed */
X            s = 0;   
N            break;
N          }
N          case '0': case '1': case '2': case '3':
