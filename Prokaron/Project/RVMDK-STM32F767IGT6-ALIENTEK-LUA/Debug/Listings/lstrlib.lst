N/*
N** Copy 'size' bytes from 'src' to 'dest', correcting endianness if
N** given 'islittle' is different from native endianness.
N*/
Nstatic void copywithendian (volatile char *dest, volatile const char *src,
N                            int size, int islittle) {
N  if (islittle == nativeendian.little) {
N    while (size-- != 0)
N      *(dest++) = *(src++);
N  }
N  else {
N    dest += size - 1;
N    while (size-- != 0)
N      *(dest--) = *(src++);
N  }
N}
