N    if (align > h->maxalign)  /* enforce maximum alignment */
N      align = h->maxalign;
N    if ((align & (align - 1)) != 0)  /* is 'align' not a power of 2? */
N      luaL_argerror(h->L, 1, "format asks for alignment not power of 2");
