X  buff[islittle ? 0 : size - 1] = (char)(n & ((1 << 8) - 1));   
N  for (i = 1; i < size; i++) {
N    n >>= NB;
X    n >>= 8;
N    buff[islittle ? i : size - 1 - i] = (char)(n & MC);
X    buff[islittle ? i : size - 1 - i] = (char)(n & ((1 << 8) - 1));
N  }
N  if (neg && size > SZINT) {  /* negative number need sign extension? */
X  if (neg && size > ((int)sizeof(lua_Integer))) {   
N    for (i = SZINT; i < size; i++)  /* correct extra bytes */
X    for (i = ((int)sizeof(lua_Integer)); i < size; i++)   
N      buff[islittle ? i : size - 1 - i] = (char)MC;
X      buff[islittle ? i : size - 1 - i] = (char)((1 << 8) - 1);
