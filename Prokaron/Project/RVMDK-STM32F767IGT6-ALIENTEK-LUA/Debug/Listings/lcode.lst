NOP_EQI,/*	A sB	if ((R(A) == sB) ~= k) then pc++		*/
NOP_LTI,/*	A sB	if ((R(A) < sB) ~= k) then pc++			*/
NOP_LEI,/*	A sB	if ((R(A) <= sB) ~= k) then pc++		*/
NOP_GTI,/*	A sB	if ((R(A) > sB) ~= k) then pc++			*/
NOP_GEI,/*	A sB	if ((R(A) >= sB) ~= k) then pc++		*/
N
