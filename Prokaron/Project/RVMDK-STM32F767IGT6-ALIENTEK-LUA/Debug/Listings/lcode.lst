S  GCObject *allweak;  /* list of all-weak tables */
S  GCObject *protogray;  /* list of prototypes with "new" caches */
S  GCObject *tobefnz;  /* list of userdata to be GC */
S  GCObject *fixedgc;  /* list of objects not to be collected */
