Sstatic const char *getstring_aux (lua_State *L, char *buff, const char **pc) {
S  int i = 0;
S  skip(pc);
S  if (**pc == '"' || **pc == '\'') {  /* quoted string? */
S    int quote = *(*pc)++;
S    while (**pc != quote) {
S      if (**pc == '\0') luaL_error(L, "unfinished string in C script");
S      buff[i++] = *(*pc)++;
S    }
S    (*pc)++;
S  }
S  else {
S    while (**pc != '\0' && !strchr(delimits, **pc))
S      buff[i++] = *(*pc)++;
S  }
S  buff[i] = '\0';
S  return buff;
S}
