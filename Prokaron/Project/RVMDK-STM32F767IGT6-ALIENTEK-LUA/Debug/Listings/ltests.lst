Sstatic void checkgraylist (global_State *g, GCObject *o) {
S  ((void)g);  /* better to keep it available if we need to print an object */
S  while (o) {
S    lua_assert(isgray(o) || getage(o) == G_TOUCHED2);
S    lua_assert(!testbit(o->marked, TESTGRAYBIT));
S    l_setbit(o->marked, TESTGRAYBIT);
S    switch (o->tt) {
S      case LUA_TTABLE: o = gco2t(o)->gclist; break;
S      case LUA_TLCL: o = gco2lcl(o)->gclist; break;
S      case LUA_TCCL: o = gco2ccl(o)->gclist; break;
S      case LUA_TTHREAD: o = gco2th(o)->gclist; break;
S      case LUA_TPROTO: o = gco2p(o)->gclist; break;
S      default: lua_assert(0);  /* other objects cannot be in a gray list */
S    }
S  }
S}
