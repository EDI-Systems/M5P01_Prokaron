S/*
S** Check consistency of an object:
S** - Dead objects can only happen in the 'allgc' list during a sweep
S** phase (controled by the caller through 'maybedead').
S** - During pause, all objects must be white.
S** - In generational mode:
S**   * objects must be old enough for their lists ('listage').
S**   * old objects cannot be white.
S**   * old objects must be black, except for 'touched1', 'old0',
S** threads, and open upvalues.
S*/
Sstatic void checkobject (global_State *g, GCObject *o, int maybedead,
S                         int listage) {
S  if (isdead(g, o))
S    lua_assert(maybedead);
S  else {
S    lua_assert(g->gcstate != GCSpause || iswhite(o));
S    if (g->gckind == KGC_GEN) {  /* generational mode? */
S      lua_assert(getage(o) >= listage);
S      lua_assert(!iswhite(o) || !isold(o));
S      if (isold(o)) {
S        lua_assert(isblack(o) ||
S        getage(o) == G_TOUCHED1 ||
S        getage(o) == G_OLD0 ||
S        o->tt == LUA_TTHREAD ||
S        (o->tt == LUA_TPROTO &&
S            (gco2p(o)->cache != NULL || gco2p(o)->cachemiss >= MAXMISS)) ||
S        (o->tt == LUA_TUPVAL && upisopen(gco2upv(o))));
S      }
S    }
S    checkrefs(g, o);
S  }
S}
