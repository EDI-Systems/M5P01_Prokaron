Sstruct Aux { jmp_buf jb; const char *paniccode; lua_State *L; };
S
S/*
S** does a long-jump back to "main program".
S*/
Sstatic int panicback (lua_State *L) {
S  struct Aux *b;
S  lua_checkstack(L, 1);  /* open space for 'Aux' struct */
S  lua_getfield(L, LUA_REGISTRYINDEX, "_jmpbuf");  /* get 'Aux' struct */
S  b = (struct Aux *)lua_touserdata(L, -1);
S  lua_pop(L, 1);  /* remove 'Aux' struct */
S  runC(b->L, L, b->paniccode);  /* run optional panic code */
S  longjmp(b->jb, 1);
S  return 1;  /* to avoid warnings */
S}
S
Sstatic int checkpanic (lua_State *L) {
S  struct Aux b;
S  void *ud;
S  lua_State *L1;
S  const char *code = luaL_checkstring(L, 1);
S  lua_Alloc f = lua_getallocf(L, &ud);
S  b.paniccode = luaL_optstring(L, 2, "");
S  b.L = L;
S  L1 = lua_newstate(f, ud);  /* create new state */
S  if (L1 == NULL) {  /* error? */
S    lua_pushnil(L);
S    return 1;
S  }
S  lua_atpanic(L1, panicback);  /* set its panic function */
S  lua_pushlightuserdata(L1, &b);
S  lua_setfield(L1, LUA_REGISTRYINDEX, "_jmpbuf");  /* store 'Aux' struct */
S  if (setjmp(b.jb) == 0) {  /* set jump buffer */
S    runC(L, L1, code);  /* run code unprotected */
S    lua_pushliteral(L, "no errors");
S  }
S  else {  /* error handling */
S    /* move error message to original state */
S    lua_pushstring(L, lua_tostring(L1, -1));
S  }
S  lua_close(L1);
S  return 1;
S}
