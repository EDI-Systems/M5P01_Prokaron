Sstatic char *buildop (Proto *p, int pc, char *buff) {
S  Instruction i = p->code[pc];
S  OpCode o = GET_OPCODE(i);
S  const char *name = luaP_opnames[o];
S  int line = luaG_getfuncline(p, pc);
S  sprintf(buff, "(%4d) %4d - ", line, pc);
S  switch (getOpMode(o)) {
S    case iABC:
S      sprintf(buff+strlen(buff), "%-12s%4d %4d %4d%s", name,
S              GETARG_A(i), GETARG_B(i), GETARG_C(i),
S              GETARG_k(i) ? " (k)" : "");
S      break;
S    case iABx:
S      sprintf(buff+strlen(buff), "%-12s%4d %4d", name, GETARG_A(i),
S                                                       GETARG_Bx(i));
S      break;
S    case iAsBx:
S      sprintf(buff+strlen(buff), "%-12s%4d %4d", name, GETARG_A(i),
S                                                       GETARG_sBx(i));
S      break;
S    case iAx:
S      sprintf(buff+strlen(buff), "%-12s%4d", name, GETARG_Ax(i));
S      break;
S    case isJ:
S      sprintf(buff+strlen(buff), "%-12s%4d (%1d)", name, GETARG_sJ(i),
S                                                         !!GETARG_m(i));
S      break;
S  }
S  return buff;
S}
