Sstatic int coresume (lua_State *L) {
S  int status, nres;
S  lua_State *co = lua_tothread(L, 1);
S  luaL_argcheck(L, co, 1, "coroutine expected");
S  status = lua_resume(co, L, 0, &nres);
S  if (status != LUA_OK && status != LUA_YIELD) {
S    lua_pushboolean(L, 0);
S    lua_insert(L, -2);
S    return 2;  /* return false + error message */
S  }
S  else {
S    lua_pushboolean(L, 1);
S    return 1;
S  }
S}
