Sstatic int getnum_aux (lua_State *L, lua_State *L1, const char **pc) {
S  int res = 0;
S  int sig = 1;
S  skip(pc);
S  if (**pc == '.') {
S    res = cast_int(lua_tointeger(L1, -1));
S    lua_pop(L1, 1);
S    (*pc)++;
S    return res;
S  }
S  else if (**pc == '*') {
S    res = lua_gettop(L1);
S    (*pc)++;
S    return res;
S  }
S  else if (**pc == '-') {
S    sig = -1;
S    (*pc)++;
S  }
S  if (!lisdigit(cast_uchar(**pc)))
S    luaL_error(L, "number expected (%s)", *pc);
S  while (lisdigit(cast_uchar(**pc))) res = res*10 + (*(*pc)++) - '0';
S  return sig*res;
S}
