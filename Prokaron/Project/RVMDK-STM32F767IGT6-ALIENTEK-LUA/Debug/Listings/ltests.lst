Sstatic int getindex_aux (lua_State *L, lua_State *L1, const char **pc) {
S  skip(pc);
S  switch (*(*pc)++) {
S    case 'R': return LUA_REGISTRYINDEX;
S    case 'G': return luaL_error(L, "deprecated index 'G'");
S    case 'U': return lua_upvalueindex(getnum_aux(L, L1, pc));
