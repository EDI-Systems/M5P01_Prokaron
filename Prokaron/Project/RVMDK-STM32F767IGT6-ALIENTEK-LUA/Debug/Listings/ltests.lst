S    block--;  /* go to real header */
S    type = block->d.type;
S    lua_assert(oldsize == block->d.size);
S  }
S  if (size == 0) {
S    freeblock(mc, block);
S    return NULL;
S  }
S  if (mc->countlimit != ~0UL && size > 0) {  /* count limit in use? */
S    if (mc->countlimit == 0)
S      return NULL;  /* fake a memory allocation error */
S    mc->countlimit--;
S  }
S  if (size > oldsize && mc->total+size-oldsize > mc->memlimit)
S    return NULL;  /* fake a memory allocation error */
S  else {
S    Header *newblock;
S    int i;
S    size_t commonsize = (oldsize < size) ? oldsize : size;
S    size_t realsize = sizeof(Header) + size + MARKSIZE;
S    if (realsize < size) return NULL;  /* arithmetic overflow! */
S    newblock = cast(Header *, malloc(realsize));  /* alloc a new block */
S    if (newblock == NULL) return NULL;  /* really out of memory? */
S    if (block) {
S      memcpy(newblock + 1, block + 1, commonsize);  /* copy old contents */
S      freeblock(mc, block);  /* erase (and check) old copy */
S    }
S    /* initialize new part of the block with something weird */
S    fillmem(cast_charp(newblock + 1) + commonsize, size - commonsize);
S    /* initialize marks after block */
S    for (i = 0; i < MARKSIZE; i++)
S      *(cast_charp(newblock + 1) + size + i) = MARK;
S    newblock->d.size = size;
S    newblock->d.type = type;
S    mc->total += size;
S    if (mc->total > mc->maxmem)
S      mc->maxmem = mc->total;
S    mc->numblocks++;
S    mc->objcount[type]++;
S    return newblock + 1;
S  }
S}
