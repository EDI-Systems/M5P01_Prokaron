Sint lua_checkmemory (lua_State *L) {
S  global_State *g = G(L);
S  GCObject *o;
S  int maybedead;
S  if (keepinvariant(g)) {
S    lua_assert(!iswhite(g->mainthread));
S    lua_assert(!iswhite(gcvalue(&g->l_registry)));
S  }
S  lua_assert(!isdead(g, gcvalue(&g->l_registry)));
S  lua_assert(g->sweepgc == NULL || issweepphase(g));
S  markgrays(g);
S
S  /* check 'fixedgc' list */
S  for (o = g->fixedgc; o != NULL; o = o->next) {
S    lua_assert(o->tt == LUA_TSHRSTR && isgray(o) && getage(o) == G_OLD);
S  }
S
S  /* check 'allgc' list */
S  checkgray(g, g->allgc);
S  maybedead = (GCSatomic < g->gcstate && g->gcstate <= GCSswpallgc);
S  checklist(g, maybedead, 0, g->allgc, g->survival, g->old, g->reallyold);
S
S  /* check 'finobj' list */
S  checkgray(g, g->finobj);
S  checklist(g, 0, 1, g->finobj, g->finobjsur, g->finobjold, g->finobjrold);
S
S  /* check 'tobefnz' list */
S  checkgray(g, g->tobefnz);
S  for (o = g->tobefnz; o != NULL; o = o->next) {
S    checkobject(g, o, 0, G_NEW);
S    lua_assert(tofinalize(o));
S    lua_assert(o->tt == LUA_TUSERDATA || o->tt == LUA_TTABLE);
S  }
S  return 0;
S}
