S/*
S** C hook that runs the C script stored in registry.C_HOOK[L]
S*/
Sstatic void Chook (lua_State *L, lua_Debug *ar) {
S  const char *scpt;
S  const char *const events [] = {"call", "ret", "line", "count", "tailcall"};
S  lua_getfield(L, LUA_REGISTRYINDEX, "C_HOOK");
S  lua_pushlightuserdata(L, L);
S  lua_gettable(L, -2);  /* get C_HOOK[L] (script saved by sethookaux) */
S  scpt = lua_tostring(L, -1);  /* not very religious (string will be popped) */
S  lua_pop(L, 2);  /* remove C_HOOK and script */
S  lua_pushstring(L, events[ar->event]);  /* may be used by script */
S  lua_pushinteger(L, ar->currentline);  /* may be used by script */
S  runC(L, L, scpt);  /* run script from C_HOOK[L] */
S}
S
S
S/*
S** sets 'registry.C_HOOK[L] = scpt' and sets 'Chook' as a hook
S*/
Sstatic void sethookaux (lua_State *L, int mask, int count, const char *scpt) {
S  if (*scpt == '\0') {  /* no script? */
S    lua_sethook(L, NULL, 0, 0);  /* turn off hooks */
S    return;
S  }
S  lua_getfield(L, LUA_REGISTRYINDEX, "C_HOOK");  /* get C_HOOK table */
S  if (!lua_istable(L, -1)) {  /* no hook table? */
S    lua_pop(L, 1);  /* remove previous value */
S    lua_newtable(L);  /* create new C_HOOK table */
S    lua_pushvalue(L, -1);
S    lua_setfield(L, LUA_REGISTRYINDEX, "C_HOOK");  /* register it */
S  }
S  lua_pushlightuserdata(L, L);
S  lua_pushstring(L, scpt);
S  lua_settable(L, -3);  /* C_HOOK[L] = script */
S  lua_sethook(L, Chook, mask, count);
S}
S
S
Sstatic int sethook (lua_State *L) {
S  if (lua_isnoneornil(L, 1))
S    lua_sethook(L, NULL, 0, 0);  /* turn off hooks */
S  else {
S    const char *scpt = luaL_checkstring(L, 1);
S    const char *smask = luaL_checkstring(L, 2);
S    int count = cast_int(luaL_optinteger(L, 3, 0));
S    int mask = 0;
S    if (strchr(smask, 'c')) mask |= LUA_MASKCALL;
S    if (strchr(smask, 'r')) mask |= LUA_MASKRET;
S    if (strchr(smask, 'l')) mask |= LUA_MASKLINE;
S    if (count > 0) mask |= LUA_MASKCOUNT;
S    sethookaux(L, mask, count, scpt);
S  }
S  return 0;
S}
