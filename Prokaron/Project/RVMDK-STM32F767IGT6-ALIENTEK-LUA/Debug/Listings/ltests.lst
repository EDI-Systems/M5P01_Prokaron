Sstatic void markgrays (global_State *g) {
S  if (!keepinvariant(g)) return;
S  checkgraylist(g, g->gray);
S  checkgraylist(g, g->grayagain);
S  checkgraylist(g, g->weak);
S  checkgraylist(g, g->ephemeron);
S  checkgraylist(g, g->protogray);
S}
