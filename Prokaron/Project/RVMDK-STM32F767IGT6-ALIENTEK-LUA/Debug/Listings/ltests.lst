N
N/* "in top" (uses top from previous instruction) */
N#define isIT(i)		(testITMode(GET_OPCODE(i)) && GETARG_B(i) == 0)
N
N#define opmode(ot,it,t,a,m) (((ot)<<6) | ((it)<<5) | ((t)<<4) | ((a)<<3) | (m))
N
N
