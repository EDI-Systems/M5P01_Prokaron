Sstatic void skip (const char **pc) {
S  for (;;) {
S    if (**pc != '\0' && strchr(delimits, **pc)) (*pc)++;
S    else if (**pc == '#') {
S      while (**pc != '\n' && **pc != '\0') (*pc)++;
S    }
S    else break;
S  }
S}
