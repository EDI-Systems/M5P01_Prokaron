Sstatic void checkgray (global_State *g, GCObject *o) {
S  for (; o != NULL; o = o->next) {
S    if ((isgray(o) && o->tt != LUA_TUPVAL) || getage(o) == G_TOUCHED2) {
S      lua_assert(!keepinvariant(g) || testbit(o->marked, TESTGRAYBIT));
S      resetbit(o->marked, TESTGRAYBIT);
S    }
S    lua_assert(!testbit(o->marked, TESTGRAYBIT));
S  }
S}
