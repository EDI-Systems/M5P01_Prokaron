Sstatic void checklist (global_State *g, int maybedead, int tof,
S  GCObject *newl, GCObject *survival, GCObject *old, GCObject *reallyold) {
S  GCObject *o;
S  for (o = newl; o != survival; o = o->next) {
S    checkobject(g, o, maybedead, G_NEW);
S    lua_assert(!tof == !tofinalize(o));
S  }
S  for (o = survival; o != old; o = o->next) {
S    checkobject(g, o, 0, G_SURVIVAL);
S    lua_assert(!tof == !tofinalize(o));
S  }
S  for (o = old; o != reallyold; o = o->next) {
S    checkobject(g, o, 0, G_OLD1);
S    lua_assert(!tof == !tofinalize(o));
S  }
S  for (o = reallyold; o != NULL; o = o->next) {
S    checkobject(g, o, 0, G_OLD);
S    lua_assert(!tof == !tofinalize(o));
S  }
S}
