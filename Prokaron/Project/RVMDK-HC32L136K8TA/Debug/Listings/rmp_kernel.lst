N                LSB=RMP_LSB_Get(Mem->Bitmap[Word]);
N                *FLI_Level=((Word<<RMP_WORD_ORDER)+LSB)>>3;
X                *FLI_Level=((Word<<5)+LSB)>>3;
N                *SLI_Level=LSB&0x07;
N                return 0;
N            }
N            else
N                RMP_COVERAGE_MARKER();
X                do { } while(0);
N        }
N    }
