S#define RMP_MAT_SPOS(MAT,POS)       ((MAT)[(POS)>>3]&(1<<((POS)&0x07)))
S#define RMP_CBOX_CHECK              (1)
S#define RMP_CBTN_DOWN               (1)
S#define RMP_RBTN_SEL                (1)
