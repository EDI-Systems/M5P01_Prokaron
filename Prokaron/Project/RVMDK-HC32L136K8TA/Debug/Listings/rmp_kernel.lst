N        for(Word=(Level>>RMP_WORD_ORDER)+1;Word<Limit;Word++)
X        for(Word=(Level>>5)+1;Word<Limit;Word++)
N        {
N            /* If the level has blocks of one FLI level */
N            if(Mem->Bitmap[Word]!=0)
N            {
N                RMP_COVERAGE_MARKER();
X                do { } while(0);
