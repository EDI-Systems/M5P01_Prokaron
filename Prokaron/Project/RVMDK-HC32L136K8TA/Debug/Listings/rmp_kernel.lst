Svoid RMP_Matrix(rmp_cnt_t Coord_X, rmp_cnt_t Coord_Y, const rmp_u8_t* Matrix,
S                rmp_cnt_t Bit_Order, rmp_cnt_t Length, rmp_cnt_t Width, rmp_ptr_t Color)
S{
S    rmp_cnt_t Len_Cnt;
S    rmp_cnt_t Wid_Cnt;
