S        {
S            for(Len_Cnt=Coord_X;Len_Cnt<Length+Coord_X;Len_Cnt++)
S            {
S                if(RMP_MAT_BPOS(Matrix,Mat_Pos)!=0)
S                    RMP_Point(Len_Cnt,Wid_Cnt,Color);
S                Mat_Pos++;
S            }
S        }
S    }
S    /* Small order */
S    else
S    {
S        for(Wid_Cnt=Coord_Y;Wid_Cnt<Width+Coord_Y;Wid_Cnt++)
S        {
S            for(Len_Cnt=Coord_X;Len_Cnt<Length+Coord_X;Len_Cnt++)
